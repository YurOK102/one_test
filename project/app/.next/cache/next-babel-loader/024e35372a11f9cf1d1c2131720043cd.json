41\\u044F \\u0441 \\u044D\\u0442\\u043E\\u0439 \\u0437\\u0430\\u0434\\u0430\\u0447\\u0435\\u0439 \\u0441\\u0430\\u043C\\u0438, \\u0435\\u0441\\u043B\\u0438 \\u043A\\u0440\\u044E\\u0447\\u043A\\u0438 \\u0438\\u043B\\u0438 \\u0441\\u0435\\u0442\\u043A\\u0443 \\u0440\\u0430\\u0437\\u043C\\u0435\\u0441\\u0442\\u0438\\u0442\\u044C \\u0434\\u043E \\u0441\\u0430\\u043C\\u043E\\u0433\\u043E \\u043F\\u043E\\u043B\\u0430. \\u041A\\u0443\\u0445\\u043D\\u044F \\u0432 \\u0441\\u0442\\u0438\\u043B\\u0435 \\\"Casual\\\" - \\u044D\\u0442\\u043E, \\u043A\\u043E\\u043D\\u0435\\u0447\\u043D\\u043E, \\u0434\\u0435\\u0440\\u0435\\u0432\\u043E. \\u0425\\u043E\\u0442\\u044F \\u043F\\u043E\\u0434\\u043E\\u0439\\u0434\\u0435\\u0442 \\u0438 \\u043F\\u043B\\u0430\\u0441\\u0442\\u0438\\u043A, \\u0435\\u0441\\u043B\\u0438 \\u0432\\u044B \\u0434\\u043E\\u043F\\u043E\\u043B\\u043D\\u0438\\u0442\\u0435 \\u0438\\u043D\\u0442\\u0435\\u0440\\u044C\\u0435\\u0440 \\u043A\\u043B\\u0435\\u0442\\u0447\\u0430\\u0442\\u044B\\u043C\\u0438, \\u043F\\u043E\\u043B\\u043E\\u0441\\u0430\\u0442\\u044B\\u043C\\u0438, \\u0432 \\u0433\\u043E\\u0440\\u043E\\u0448\\u0435\\u043A \\u0438\\u043B\\u0438 \\u0446\\u0432\\u0435\\u0442\\u043E\\u0447\\u0435\\u043A \\u0448\\u0442\\u043E\\u0440\\u0430\\u043C\\u0438 \\u0438 \\u0441\\u043A\\u0430\\u0442\\u0435\\u0440\\u044C\\u044E. \\u041C\\u043E\\u0436\\u043D\\u043E \\u0443\\u043A\\u0440\\u0430\\u0441\\u0438\\u0442\\u044C \\u043F\\u043E \\u0432\\u0430\\u0448\\u0435\\u043C\\u0443 \\u0440\\u0430\\u0437\\u0443\\u043C\\u0435\\u043D\\u0438\\u044E \\u043C\\u043D\\u043E\\u0436\\u0435\\u0441\\u0442\\u0432\\u043E\\u043C \\u0440\\u0430\\u0437\\u043D\\u043E\\u0446\\u0432\\u0435\\u0442\\u043D\\u044B\\u0445 \\u0434\\u0435\\u0442\\u0430\\u043B\\u0435\\u0439: \\u043A\\u0440\\u0443\\u0436\\u0435\\u0447\\u0435\\u043A, \\u0431\\u0430\\u043D\\u043E\\u0447\\u0435\\u043A, \\u043F\\u043B\\u043E\\u0448\\u0435\\u043A. \\u0422\\u043E\\u043B\\u044C\\u043A\\u043E \\u043D\\u0435 \\u043F\\u0435\\u0440\\u0435\\u0443\\u0441\\u0435\\u0440\\u0434\\u0441\\u0442\\u0432\\u0443\\u0439\\u0442\\u0435, \\u0432\\u0435\\u0434\\u044C \\\"casual\\\", \\u043F\\u0440\\u0435\\u0436\\u0434\\u0435 \\u0432\\u0441\\u0435\\u0433\\u043E \\u0441\\u0432\\u043E\\u0431\\u043E\\u0434\\u0430, \\u043F\\u0440\\u043E\\u0441\\u0442\\u043E\\u0440, \\u0430 \\u043D\\u0435 \\u043F\\u043E\\u043C\\u0435\\u0449\\u0435\\u043D\\u0438\\u0435 \\u043F\\u043B\\u043E\\u0442\\u043D\\u043E \\u0437\\u0430\\u0431\\u0438\\u0442\\u043E\\u0435 \\u0432\\u0435\\u0449\\u0430\\u043C\\u0438.\"));\n}\n\n_c = InfoText;\nexport default InfoText;\n\nvar _c;\n\n$RefreshReg$(_c, \"InfoText\");","map":{"version":3,"sources":["D:/web/works/react/project/app/src/components/InfoText.js"],"names":["React","InfoText"],"mappings":";;AAAA,OAAOA,KAAP,MAAkB,OAAlB;;AAEA,SAASC,QAAT,GAAoB;AAClB,SACE;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAQ,IAAA,SAAS,EAAC,iBAAlB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gEADF,EAED;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,i1JAFC,CADF;AAWD;;KAZQA,Q;AAcT,eAAeA,QAAf","sourcesContent":["import React from \"react\";\r\n\r\nfunction InfoText() {\r\n  return (\r\n    <div className=\"description\">\r\n      <button className=\"description-btn\"> Вернуться </button>\r\n\t\t\t<div>\r\n\t\t\t\t\"Casual\" - это свободный и динамичный стиль, причем он используется не только в одежде, но и в интерьере.\r\n\t\t\t\tНа рабочем месте это правильное распределение пространства, удобная деревянная мебель, натуральные ткани, а дома, в вашей квартире - это, в первую очередь, удобство и уют и много свободного пространства.\r\n\t\t\t\tПростым решением для вашей прихожей может быть система крючков, установленных на деревянной плите или натянута сетка, на которую можно вешать все, что угодно: обувь за каблуки, сумки, зонты, пакеты, одежду. Причем, даже дети спокойно справятся с этой задачей сами, если крючки или сетку разместить до самого пола.\r\n\t\t\t\tКухня в стиле \"Casual\" - это, конечно, дерево. Хотя подойдет и пластик, если вы дополните интерьер клетчатыми, полосатыми, в горошек или цветочек шторами и скатерью. Можно украсить по вашему разумению множеством разноцветных деталей: кружечек, баночек, плошек. Только не переусердствуйте, ведь \"casual\", прежде всего свобода, простор, а не помещение плотно забитое вещами.\r\n\t\t\t</div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default InfoText;\r\n"]},"metadata":{},"sourceType":"module"}