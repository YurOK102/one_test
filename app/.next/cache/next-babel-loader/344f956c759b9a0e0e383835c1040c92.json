D\\u0442\\u0435\\u0440\\u044C\\u0435\\u0440 \\u043A\\u043B\\u0435\\u0442\\u0447\\u0430\\u0442\\u044B\\u043C\\u0438, \\u043F\\u043E\\u043B\\u043E\\u0441\\u0430\\u0442\\u044B\\u043C\\u0438, \\u0432 \\u0433\\u043E\\u0440\\u043E\\u0448\\u0435\\u043A \\u0438\\u043B\\u0438 \\u0446\\u0432\\u0435\\u0442\\u043E\\u0447\\u0435\\u043A \\u0448\\u0442\\u043E\\u0440\\u0430\\u043C\\u0438 \\u0438 \\u0441\\u043A\\u0430\\u0442\\u0435\\u0440\\u044C\\u044E. \\u041C\\u043E\\u0436\\u043D\\u043E \\u0443\\u043A\\u0440\\u0430\\u0441\\u0438\\u0442\\u044C \\u043F\\u043E \\u0432\\u0430\\u0448\\u0435\\u043C\\u0443 \\u0440\\u0430\\u0437\\u0443\\u043C\\u0435\\u043D\\u0438\\u044E \\u043C\\u043D\\u043E\\u0436\\u0435\\u0441\\u0442\\u0432\\u043E\\u043C \\u0440\\u0430\\u0437\\u043D\\u043E\\u0446\\u0432\\u0435\\u0442\\u043D\\u044B\\u0445 \\u0434\\u0435\\u0442\\u0430\\u043B\\u0435\\u0439: \\u043A\\u0440\\u0443\\u0436\\u0435\\u0447\\u0435\\u043A, \\u0431\\u0430\\u043D\\u043E\\u0447\\u0435\\u043A, \\u043F\\u043B\\u043E\\u0448\\u0435\\u043A. \\u0422\\u043E\\u043B\\u044C\\u043A\\u043E \\u043D\\u0435 \\u043F\\u0435\\u0440\\u0435\\u0443\\u0441\\u0435\\u0440\\u0434\\u0441\\u0442\\u0432\\u0443\\u0439\\u0442\\u0435, \\u0432\\u0435\\u0434\\u044C \\\"casual\\\", \\u043F\\u0440\\u0435\\u0436\\u0434\\u0435 \\u0432\\u0441\\u0435\\u0433\\u043E \\u0441\\u0432\\u043E\\u0431\\u043E\\u0434\\u0430, \\u043F\\u0440\\u043E\\u0441\\u0442\\u043E\\u0440, \\u0430 \\u043D\\u0435 \\u043F\\u043E\\u043C\\u0435\\u0449\\u0435\\u043D\\u0438\\u0435 \\u043F\\u043B\\u043E\\u0442\\u043D\\u043E \\u0437\\u0430\\u0431\\u0438\\u0442\\u043E\\u0435 \\u0432\\u0435\\u0449\\u0430\\u043C\\u0438.\"));\n}\n\nexport default InfoText;","map":{"version":3,"sources":["D:/web/works/react/project/app/src/components/InfoText.js"],"names":["React","useState","InfoText","close","setClose"],"mappings":";;AAAA,OAAOA,KAAP,IAAgBC,QAAhB,QAAgC,OAAhC;;AAEA,SAASC,QAAT,GAAoB;AAElB,QAAM;AAAA,OAACC,KAAD;AAAA,OAAQC;AAAR,MAAoBH,QAAQ,CAAC,KAAD,CAAlC;AAGA,SACE;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAQ,IAAA,SAAS,EAAC,iBAAlB;AAAoC,IAAA,OAAO,EAAE,MAAMG,QAAQ,CAAC,CAACD,KAAF,CAA3D;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,GAAG,EAAC,mBAAT;AAA6B,IAAA,SAAS,EAAC,YAAvC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,2DADF,EAKE;AAAK,IAAA,SAAS,EAAC,kBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ifAEqC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAFrC,shCAKkD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IALlD,EAME;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IANF,2jDAWuC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAXvC,EAYE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAZF,kyDALF,CADF;AA4BD;;AAED,eAAeD,QAAf","sourcesContent":["import React, { useState } from \"react\";\r\n\r\nfunction InfoText() {\r\n\r\n  const [close, setClose] = useState(false);\r\n  \r\n\r\n  return (\r\n    <div className=\"description\">\r\n      <button className=\"description-btn\" onClick={() => setClose(!close)}>\r\n        <img src=\"/images/arrow.svg\" className=\"arrow-icon\" />\r\n        Вернуться\r\n      </button>\r\n      <div className=\"description-text\">\r\n        \"Casual\" - это свободный и динамичный стиль, причем он используется не\r\n        только в одежде, но и в интерьере. <br /> На рабочем месте это\r\n        правильное распределение пространства, удобная деревянная мебель,\r\n        натуральные ткани, а дома, в вашей квартире - это, в первую очередь,\r\n        удобство и уют и много свободного пространства. <br />\r\n        <br />\r\n        Простым решением для вашей прихожей может быть система крючков,\r\n        установленных на деревянной плите или натянута сетка, на которую можно\r\n        вешать все, что угодно: обувь за каблуки, сумки, зонты, пакеты, одежду.\r\n        Причем, даже дети спокойно справятся с этой задачей сами, если крючки\r\n        или сетку разместить до самого пола. <br />\r\n        <br />\r\n        Кухня в стиле \"Casual\" - это, конечно, дерево. Хотя подойдет и пластик,\r\n        если вы дополните интерьер клетчатыми, полосатыми, в горошек или\r\n        цветочек шторами и скатерью. Можно украсить по вашему разумению\r\n        множеством разноцветных деталей: кружечек, баночек, плошек. Только не\r\n        переусердствуйте, ведь \"casual\", прежде всего свобода, простор, а не\r\n        помещение плотно забитое вещами.\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default InfoText;\r\n"]},"metadata":{},"sourceType":"module"}